{
  "name": "mern-landing-page",
  "version": "1.0.0",
  "description": "MERN landing page app",
  "engines": {
    "node": "16.15.1"
  },
  "main": "server.js",
  "type": "module",
  "scripts": {
    "install-client": "cd .. && cd client && npm install --legacy-peer-deps",
    "build": "cd .. && cd client && npm run build",
    "start": "node index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm run install-client && npm run build --prefix client"
    },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-handler": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.5.1",
    "multer": "^1.4.5-lts.1",
    "uuid": "^8.3.2"
  }
}
